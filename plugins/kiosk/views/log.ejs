<div class="row log">
	<div class="span12">
		<% for (var i = log.length -1; i >= 0; i--) { %>
		<div class="row">
			<div class="span12">
				<span class="time"><time><%= format.isodate(log[i].time) %></time></span>
				<span class="user">
				<% if (users[log[i].user]) { %>
				<%= users[log[i].user].fullName() %>
				<% } else { %>
				<em>Unknown</em>
				<% } %>
				</span>
				<span class="account">
				<% if (users[log[i].accountId]) { %>
				<a href="/account/<%= log[i].accountId %>"><%= users[log[i].accountId].fullName() %></a>
				<% } else { %>
				<em>Unknown</em>
				<% } %>
				</span>
				<% if (log[i].booking) { %>
				<span class="label label-success"><%= log[i].booking.type %></span>
				<% } %>
				<% if (log[i].booking && log[i].booking.automatic) { %>
				<span class="label">automatic</span>
				<% } %>
				<% if (log[i].booking && log[i].booking.admin) { %>
				<span class="label label-info">admin</span>
				<% } %>
				<% if (log[i].booking && log[i].booking.reversed) { %>
				<span class="label label-important">reversed</span>
				<% } %>
				<% if (log[i].booking) { %>
				<div class="booking">
					<pre class="prettyprint"><%= JSON.stringify(log[i].booking, null, '\t') %></pre>
				</div>
				<% } %>
			</div>
		</div>
		<% } %>
	</div>
</div>